<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.catquiz">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivityE1" />
        <activity android:name=".MainActivityM1" />
        <activity android:name=".MainActivityH1" />
        <activity android:name=".ScoreActivity" />
        <activity android:name=".MainActivityE2" />
        <activity android:name=".MainActivityM2" />
        <activity android:name=".MainActivityH2"></activity>
    </application>

</manifest>